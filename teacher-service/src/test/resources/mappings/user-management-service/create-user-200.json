{
  "request": {
    "urlPath": "/api/users/username",
    "method": "PUT",
    "bodyPatterns": [
      {
        "equalToJson": {
          "emailAddress": "username@teacher.com",
          "roles": [
            "TEACHER"
          ]
        },
        "ignoreArrayOrder": true,
        "ignoreExtraElements": true
      }
    ]
  },
  "response": {
    "status": 200
  }
}
